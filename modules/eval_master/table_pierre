      'type' => 'serial',
      'unsigned' => TRUE,
      'not null' => TRUE,
    ),
    'category' => array(
      'type' => 'varchar',
      'length' => 64,
      'not null' => TRUE,
      'default' => '',
    ),
    'label' => array(
      'type' => 'varchar',
      'length' => 512,
      'not null' => TRUE,
      'default' => '',
    ),
    'type' => array(
      'type' => 'varchar',
      'length' => 64,
      'not null' => TRUE,
      'default' => '',
    ),

  ),
);
